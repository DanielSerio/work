{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "#lib/*": ["src/lib/*"],
      "#components/*": ["src/App/components/*"],
      "#hooks/*": ["src/App/hooks/*"],
      "#pages/*": ["src/App/pages/*"]
    }
  }
}
